<resources>
        <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        //改變EditText底線顏色
        <item name="colorControlNormal">#808080</item>
        <item name="colorControlActivated">#89B094</item>

    </style>


    <style name="Theme.SearchView" parent="Base.Widget.AppCompat.SearchView">
        <item name="searchIcon">@drawable/searchbar</item>
        <item name="queryHint">搜尋作物</item>
        <item name="queryBackground">@drawable/searchbar</item>
    </style>

    <style name="custem_fab">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">#E6EAE7</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
